<unity xmlns="http://schemas.microsoft.com/practices/2010/unity">

  <alias alias="IArquivoDataAccess" type="DCC.COLAB.Common.DataAccess.IArquivoDataAccess, DCC.COLAB.Common"/>
  <alias alias="ArquivoDataAccess" type="DCC.COLAB.DataAccess.SQLServer.Entities.ArquivoDataAccess, DCC.COLAB.DataAccess.SQLServer"/>

  <alias alias="IDocumentoDataAccess" type="DCC.COLAB.Common.DataAccess.IDocumentoDataAccess, DCC.COLAB.Common"/>
  <alias alias="DocumentoDataAccess" type="DCC.COLAB.DataAccess.SQLServer.Entities.DocumentoDataAccess, DCC.COLAB.DataAccess.SQLServer"/>
  
  <alias alias="IPerfilAcessoDataAccess" type="DCC.COLAB.Common.DataAccess.IPerfilAcessoDataAccess, DCC.COLAB.Common"/>
  <alias alias="PerfilAcessoDataAccess" type="DCC.COLAB.DataAccess.SQLServer.Entities.PerfilAcessoDataAccess, DCC.COLAB.DataAccess.SQLServer"/>

  <alias alias="IUsuarioDataAccess" type="DCC.COLAB.Common.DataAccess.IUsuarioDataAccess, DCC.COLAB.Common"/>
  <alias alias="UsuarioDataAccess" type="DCC.COLAB.DataAccess.SQLServer.Entities.UsuarioDataAccess, DCC.COLAB.DataAccess.SQLServer"/>

  <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Microsoft.Practices.Unity.Interception.Configuration"/>
  
  <container>
    <extension type="Interception"/>

    <register type="IArquivoDataAccess" mapTo="ArquivoDataAccess">
      <lifetime type="transient" />
      <interceptor type="VirtualMethodInterceptor" />
    </register>

    <register type="IDocumentoDataAccess" mapTo="DocumentoDataAccess">
      <lifetime type="transient" />
      <interceptor type="VirtualMethodInterceptor" />
    </register>

    <register type="IPerfilAcessoDataAccess" mapTo="PerfilAcessoDataAccess">
      <lifetime type="transient" />
      <interceptor type="VirtualMethodInterceptor" />
    </register>

    <register type="IUsuarioDataAccess" mapTo="UsuarioDataAccess">
      <lifetime type="transient" />
      <interceptor type="VirtualMethodInterceptor" />
    </register>

  </container>

</unity>